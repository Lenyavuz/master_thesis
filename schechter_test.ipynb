      ]
     },
     "metadata": {},
     "output_type": "display_data"
    }
   ],
   "source": [
    "zxcvb = plt.hist(schMr,bins=np.linspace(-30,-10,120))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 140,
   "id": "4ba0077e",
   "metadata": {},
   "outputs": [
    {
     "name": "stderr",
     "output_type": "stream",
     "text": [
      "/tmp/ipykernel_12607/3892862343.py:4: RuntimeWarning: overflow encountered in power\n",
      "  return 0.4*np.log(10)*phi*10**(-0.4*(1+alpha)*(x-M_star))*np.exp(-1*(10)**(-0.4*(x-M_star)))\n"
     ]
    }
   ],
   "source": [
    "counts, edges = np.histogram(schMr, bins=np.linspace(-30,-10,100000))\n",
    "optedges = 0.5*(edges[1:]+edges[:-1])\n",
    "coeffs, cov = curve_fit(func, optedges, counts, p0=[-21, 0.1])"
   ]
  },
